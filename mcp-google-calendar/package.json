{
  "name": "@petaio/mcp-google-calendar",
  "version": "1.0.1",
  "description": "Google Calendar MCP Server for PETA ecosystem with STDIO transport. Provides 10 powerful tools for calendar and event management.",
  "main": "dist/stdio.js",
  "type": "module",
  "bin": {
    "peta-mcp-google-calendar": "./dist/stdio.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/apps && cp -r src/apps/* dist/apps/ 2>/dev/null || true",
    "dev": "tsx watch src/stdio.ts",
    "start": "node dist/stdio.js",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:watch": "vitest watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "docker:build": "./build-docker.sh"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "google-calendar",
    "google",
    "calendar",
    "peta",
    "peta-desk",
    "stdio",
    "event-management",
    "scheduling",
    "oauth2",
    "docker",
    "typescript",
    "claude",
    "ai"
  ],
  "author": "PETA Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@modelcontextprotocol/ext-apps": "^1.0.1",
    "dotenv": "^16.4.5",
    "googleapis": "^140.0.0",
    "https-proxy-agent": "^7.0.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.7.2",
    "vitest": "^4.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
