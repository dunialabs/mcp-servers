{
  "mcpServers": {
    "google-drive-docker": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "-e",
        "accessToken",
        "-e",
        "HTTP_PROXY",
        "-e",
        "HTTPS_PROXY",
        "peta/mcp-google-drive:latest"
      ],
      "env": {
        "accessToken": "ya29.YOUR_ACCESS_TOKEN_HERE",
        "HTTP_PROXY": "http://host.docker.internal:7897",
        "HTTPS_PROXY": "http://host.docker.internal:7897"
      }
    },
    "google-drive-nodejs": {
      "command": "node",
      "args": [
        "/Users/tataufo/mcp-google-drive/dist/stdio.js"
      ],
      "env": {
        "accessToken": "ya29.YOUR_ACCESS_TOKEN_HERE",
        "HTTP_PROXY": "http://127.0.0.1:7897",
        "HTTPS_PROXY": "http://127.0.0.1:7897"
      }
    }
  }
}
