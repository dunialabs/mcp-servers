# MySQL Connection String (REQUIRED)
# Format: mysql://[user[:password]@][host][:port][/database]
MYSQL_URL=mysql://user:password@localhost:3306/database

# Logging Level: DEBUG | INFO | WARN | ERROR | NONE (default: INFO)
LOG_LEVEL=INFO

# Optional: Override default timeouts (milliseconds)
# MYSQL_CONNECT_TIMEOUT=10000
# MYSQL_QUERY_TIMEOUT=30000
